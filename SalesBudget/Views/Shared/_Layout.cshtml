<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]SalesBudget</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/startbootstrap-simple-sidebar@5.1.2/css/simple-sidebar.css" />
</head>
<body>

        <header>
            <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
                <div class="container-fluid">
                    <a class="navbar-brand" asp-area="Admin" asp-controller="Budget" asp-action="Index"></i>SalesBudget</a>
                    @*<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColo01"
                aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>*@

                    <div class="collapse navbar-collapse" id="navbarColor01">
                        <ul class="navbar-nav me-auto">
                            <li class="nav-item">
                                <a class="nav-link" asp-area="Admin" asp-controller="Budget" asp-action="Index"><i class="fas fa-home"></i> Home</a>
                            </li>
                            <li>
                                <a class="nav-link" asp-area="Admin" asp-controller="CopyFromTo" asp-action="Index" target="_blank"><i class="fas fa-file-import"></i> Copy Budget from-to</a>
                            </li>
                            <li>
                                <a class="nav-link" asp-area="Admin" asp-controller="OtherTables" asp-action="Index" target="_blank"><i class="fas fa-table"></i> Other tables</a>
                            </li>

                        </ul>
                    </div>
                    <a class="text-light" href="https://salesbudget20211202205928.azurewebsites.net/.auth/logout">Logout from Azure AD <i class="fas fa-sign-out-alt"></i></a>


                </div>
            </nav>

        </header>

        <img src="~/img/1781561.jpg" width="400" height="70"/>

        <div class="container">
            <main role="main" class="pb-0">
                @RenderBody()
